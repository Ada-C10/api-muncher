<div class="list-container">
  <h1>Results for: <%= @dish%><%=" "%><%= "(#{@recipes.length})"%></h1>
<ul>
<div class="row">
  <% @recipes.each do |recipe| %>
  <div class="col-sm">
  <li><strong><%= recipe.name %></strong></li>
  <li><%= image_tag recipe.image_url %></li>
  <li><%= link_to "See Recipe", show_recipe_path(uri: recipe.uri)%></li>
    </div>
  <%end%>
  </div>
</ul>
</div>
