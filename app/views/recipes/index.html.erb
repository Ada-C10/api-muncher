<main>
  <h2>Results</h2>
  <ul>
    <% @recipes.each do |recipe| %>
    <li>
      <%= image_tag recipe.image, alt: recipe.label %>
      <%= link_to recipe.label, recipes_path(uri: recipe.uri) %>
    </li>
    <% end %>
  </ul>
  <%= paginate @recipes %>
  <%= page_entries_info @recipes %>

</main>
