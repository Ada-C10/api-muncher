<section class="recipes-list">


  <h1>Recipes</h1>
  <% if @recipes %>
    <ul>
      <% @recipes.each do |recipe| %>
        <li>
          <%= link_to recipe.name %>
          <%= image_tag recipe.image %>

        </li>
      <% end %>
    </ul>
  <% end %>

    <%= will_paginate @posts %>
</section>
