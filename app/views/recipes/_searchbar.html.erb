<div id="search_partial_container" class="container">
  <h1>Find Your Flavor!</h1>
  <%= form_tag recipe_index_path, method: :get do %>
    <%= text_field_tag ( :search_terms ), placeholder="Find A Recipe" %>
    <%= submit_tag ( "Search" ) %>
  <% end %>
  <div id="edamam-badge" data-color="light"></div>
</div>
