<section class="recipes-list">
  <h1><%= @recipe.label %></h1>
  <div>
      <ul>
        <li><%=@recipe.label %></td>
        <li><%=link_to image_tag(@recipe.image)  %></li>
        <li><%=@recipe.yield %></li>
        <li><%=@recipe.healthLabels  %></li>
        <li><%=@recipe.ingredients  %></li>
        <li><%=@recipe.calories  %></li>
      </li>
  </ul>
</section>
