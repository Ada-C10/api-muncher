<div class="container">
  <div class="jumbotron">
    <h1 class="display-1 text-center">Muncher</h1>
  </div>

  <%= form_tag search_path, method: :get do %>
    <div class="md-form my-0">
      <%= label_tag :search, "Find Your Flavor", class: "col-form-label"%>
      <%= text_field_tag(:search, params[:search], class: "form-control mt-3", placeholder: "Search") %>
    </div>
  <% end %>
</div>
